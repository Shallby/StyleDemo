<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.styledemo">
    <supports-screens android:smallScreens="false"
            android:compatibleWidthLimitDp="320"
            android:largestWidthLimitDp="1080"
        >
    </supports-screens>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/timg"
        android:label="小米游戏笔记本"
        android:supportsRtl="true"
        android:theme="@style/Theme.StyleDemo">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <!--android:roundIcon="@mipmap/ic_launcher_round" 首先不需要圆形的图标不需要这句 这是安卓8.0出来的-->
</manifest>